{
  "name": "@kyma-project/documentation-component",
  "description": "Reusable React component compatible with markdown and most known specifications",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kyma-incubator/documentation-component/blob/master/packages/documentation-component"
  },
  "author": {
    "name": "Kyma Core & Supporting Team",
    "url": "http://kyma-project.io"
  },
  "keywords": [
    "documentation",
    "docs",
    "react",
    "component",
    "web-component",
    "open-api",
    "openapi",
    "async-api",
    "asyncapi",
    "odata",
    "markdown",
    "specification"
  ],
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "lint-check": "tslint -c ../../tslint.json --project tsconfig.json --format verbose && prettier --check '**/*.{ts,tsx,js,jsx,json,html,css,yaml}'",
    "lint-fix": "tslint -c ../../tslint.json --project tsconfig.json --format verbose --fix; prettier --write '**/*.{ts,tsx,js,jsx,json,html,css,yaml}'",
    "prepare": "npm run build"
  },
  "dependencies": {
    "constate": "^1.1.1",
    "gray-matter": "^4.0.2",
    "lodash.has": "^4.5.2",
    "lodash.isdate": "^4.0.1",
    "lodash.mapvalues": "^4.6.0",
    "prism-react-renderer": "^0.1.6",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-markdown": "^4.1.0",
    "react-player": "^1.11.1"
  },
  "devDependencies": {
    "@types/lodash.has": "^4.5.6",
    "@types/lodash.isdate": "^4.0.6",
    "@types/lodash.mapvalues": "^4.6.6",
    "@types/react": "16.8.20",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "typescript": "3.5.2"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "./README.md",
    "./LICENSE"
  ]
