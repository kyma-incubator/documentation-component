{
  "name": "@kyma-project/odata-react",
  "description": "React component for visualization OData specification",
  "version": "0.1.2",
  "license": "Apache-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kyma-incubator/documentation-component/blob/master/packages/odata-react"
  },
  "author": {
    "name": "Kyma Core & Supporting Team",
    "url": "http://kyma-project.io"
  },
  "keywords": [
    "odata",
    "specification"
  ],
  "dependencies": {
    "fiori-fundamentals": "^1.4.3",
    "fundamental-react": "0.4.0",
    "react-xml-parser": "^1.1.2",
    "tslib": "^1.10.0",
    "xslt": "^0.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.2",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.10",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "mini-css-extract-plugin": "^0.7.0",
    "prettier": "^1.16.4",
    "react": "^16.8.4",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.4",
    "react-scripts": "^2.1.4",
    "resource-hints-webpack-plugin": "^0.0.2",
    "style-loader": "^0.23.1",
    "styled-components": "^3.4.10",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "styled-components": ">= 3.4.10"
  },
  "main": "lib/ODataReact.js",
  "types": "lib/ODataReact.d.ts",
  "files": [
    "./lib/",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config=webpack.dev.js",
    "build": "webpack --config=webpack.prod.js",
    "test": "react-scripts test; exit 0",
    "eject": "react-scripts eject",
    "lint-check": "tslint -c ../../tslint.json --project tsconfig.json --format verbose && prettier --config ../../.prettierrc --check 'src/**/*.{ts,tsx,js,jsx,json,html,css,yaml}'",
    "lint-fix": "tslint -c ../../tslint.json --project tsconfig.json --format verbose --fix; prettier --config ../../.prettierrc --write 'src/**/*.{ts,tsx,js,jsx,json,html,css,yaml}'",
    "conflict-check": "tslint-config-prettier-check ../../tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json,css,html,yaml}": [
        "prettier --config ../../.prettierrc --write",
        "git add"
      ],
      "**/*.{ts,tsx}": [
        "tslint -c tslint.json --project tsconfig.json --format verbose --fix",
        "prettier --config ../../.prettierrc --write"
      ]
    },
    "ignore": [
      "package.json",
      "package-lock.json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
