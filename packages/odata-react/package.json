{
  "name": "@kyma-project/odata-react",
  "description": "React component for visualization OData specification",
  "version": "0.1.2",
  "license": "Apache-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kyma-incubator/documentation-component/blob/master/packages/odata-react"
  },
  "author": {
    "name": "Kyma Core & Supporting Team",
    "url": "http://kyma-project.io"
  },
  "keywords": [
    "odata",
    "specification"
  ],
  "dependencies": {
    "fiori-fundamentals": "^1.4.3",
    "fundamental-react": "0.4.0",
    "react-xml-parser": "^1.1.2",
    "xslt": "^0.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.0",
    "@types/node": "12.6.2",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.10",
    "babel-plugin-styled-components": "^1.10.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "3.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-visualizer": "^2.4.4",
    "styled-components": "^5.0.0-beta.8",
    "tslib": "^1.10.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "styled-components": ">= 4.2.0"
  },
  "types": "lib/ODataReact.d.ts",
  "main": "lib/ODataReact.esm.js",
  "module": "lib/ODataReact.esm.js",
  "browser": "lib/ODataReact.umd.js",
  "files": [
    "./lib/",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rimraf lib && rollup -c --compact",
    "test": "react-scripts test; exit 0",
    "lint-check": "tslint -c ../../tslint.json --project tsconfig.prod.json --format verbose && prettier --config ../../.prettierrc --check 'src/**/*.{ts,tsx,js,jsx,json,html,css,yaml}'",
    "lint-fix": "tslint -c ../../tslint.json --project tsconfig.prod.json --format verbose --fix; prettier --config ../../.prettierrc --write 'src/**/*.{ts,tsx,js,jsx,json,html,css,yaml}'",
    "conflict-check": "tslint-config-prettier-check ../../tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json,css,html,yaml}": [
        "prettier --config ../../.prettierrc --write",
        "git add"
      ],
      "**/*.{ts,tsx}": [
        "tslint -c tslint.json --project tsconfig.prod.json --format verbose --fix",
        "prettier --config ../../.prettierrc --write"
      ]
    },
    "ignore": [
      "package.json",
      "package-lock.json"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
